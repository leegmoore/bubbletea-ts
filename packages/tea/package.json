{
  "name": "@bubbletea/tea",
  "version": "0.0.0",
  "description": "Core runtime for the Bubble Tea TypeScript port",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./internal": {
      "types": "./src/internal/index.ts",
      "import": "./src/internal/index.ts"
    },
    "./internal/*": {
      "types": "./src/internal/*.ts",
      "import": "./src/internal/*.ts"
    }
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3"
  }
}
