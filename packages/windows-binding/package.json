{
  "name": "@bubbletea/windows-binding",
  "version": "0.0.0",
  "description": "Native Windows console binding for the Bubble Tea TypeScript port",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "bubbletea",
    "windows",
    "console",
    "node-addon-api"
  ],
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3"
  }
}
